(def tests
  [{:pre accounts-test1
    :view [acc-views/accounts]}
   {:pre accounts-test2
    :view [acc-views/accounts]}
   {:pre dapps-test1
    :view [dapp-views/open-dapp]}
   {:pre dapps-test2
    :view [dapp-views/open-dapp]}
   {:pre contact-list1
    :view [contacts-list-views/contacts-list]}
   {:pre contact-list2
    :view [contacts-list-views/contacts-list]}])

(defn update-subs []
  (re-frame/clear-subscription-cache!)
  (when-let [pre (get-in tests [(:id @sesamum) :pre])]
    (pre)))

(defn activate-sesamum []
  (reset! sesamum {:id 0 :labels? false})
  (update-subs))

(views/defview sesamum-view []
  {:component-will-mount activate-sesamum}
  [react/view {:flex 1}
   [react/view {:flex-direction :row}
    (if (:width @sesamum)
      [react/touchable-highlight {:on-press #(swap! sesamum dissoc :width :height :border-color :border-width)}
       [react/view {:background-color colors/gray}
        [react/text "Original"]]]
      [react/touchable-highlight {:on-press #(swap! sesamum merge {:width 320 :height 440 :border-color :red :border-width 1})}
       [react/view {:background-color colors/gray}
        [react/text "iPhone4"]]])
    [react/view {:width 5}]
    [react/touchable-highlight {:on-press #(do (swap! i18n/sesamum? not)
                                               (swap! sesamum update :labels? not))}
     [react/view {:background-color colors/gray}
      [react/text "Labels"]]]
    [react/view {:flex 1}]
    (when-not (zero? (:id @sesamum))
      [react/touchable-highlight {:on-press #(do (swap! sesamum update :id dec)
                                                 (update-subs))}
       [react/view {:background-color colors/gray :width 20 :align-items :center}
        [react/text "<"]]])
    [react/text {:style {:margin-horizontal 5}} (:id @sesamum)]
    (if (= (:id @sesamum) (dec (count tests)))
      [react/view {:width 20}]
      [react/touchable-highlight {:on-press #(do (swap! sesamum update :id inc)
                                                 (update-subs))}
       [react/view {:background-color colors/gray :width 20 :align-items :center}
        [react/text ">"]]])]
   ^{:key (str "viewkey" (:id @sesamum) (:labels? @sesamum))}
   [react/view {:style (if (:width @sesamum) @sesamum {:flex 1})}
    (:view (get tests (:id @sesamum)))]])